library(data.table)
library(ggplot2)
prop <- read.csv('../data/word_proportion_results.csv', header=FALSE)
setnames(prop, c('proportion'))
g <- ggplot(data = prop, aes(x=proportion)) + geom_histogram(color='firebrick', fill='lightblue', bins=20)
g + labs(x = 'Proportion of Words in Page', y = "")
summary(prop$proportion)
